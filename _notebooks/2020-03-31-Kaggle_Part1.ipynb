      "source": [
        "#collapse-hide\n",
        "y_pred = np.arange(0,1,0.01)\n",
        "\n",
        "def logloss(y_pred):\n",
        "  return -np.log(1-y_pred)\n",
        "\n",
        "plt.figure(figsize=(10,7))\n",
        "plt.xlabel(\"$\\hat y$\")\n",
        "plt.ylabel('Loss')\n",
        "plt.xlim(-0.01,1.01)\n",
        "plt.ylim(-0.1,6)\n",
        "plt.plot(y_pred,logloss(y_pred),label='LogLoss')\n",
        "plt.legend(loc='upper left')\n",
        "plt.show()"
      ],
